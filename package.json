{
  "name": "vuewidget",
  "version": "0.0.1",
  "description": "A Custom Jupyter Widget Library",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/kitegg/vuewidget",
  "bugs": {
    "url": "https://github.com/kitegg/vuewidget/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jean Boehm",
    "email": "jean.boehm@hs-mainz.de"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitegg/vuewidget"
  },
  "scripts": {
    "build": "yarn run build:nbextension && yarn run build:labextension:dev",
    "build:prod": "yarn run build:nbextension && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:nbextension": "webpack --mode=development",
    "clean": "yarn run clean:lib && yarn run clean:nbextension && yarn run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf vuewidget/labextension",
    "clean:nbextension": "rimraf vuewidget/nbextension/static/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "prepack": "yarn run build:prod",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10 || ^2.0.0 || ^3.0.0 || ^4.0.0",
    "@kitegg/kitegg-ui": "^1.7.4",
    "@types/vue": "^2.0.0",
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.8"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@jupyterlab/builder": "^3.0.0",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@types/jest": "^26.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@volar/vue-language-plugin-pug": "^1.0.0",
    "@vue/tsconfig": "^0.1.3",
    "acorn": "^7.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.0.5",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.0.0"
  },
  "jupyterlab": {
    "extension": "lib/index",
    "outputDir": "vuewidget/labextension/",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
